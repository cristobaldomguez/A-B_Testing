<div class="content login">
  <div class="container">
    <div class="login-container">
      <%= image_tag "default_profile.svg", class: 'image_profile' %>
      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

        <%= f.email_field :email, class: 'usr', placeholder: 'Correo', autofocus: true %>
        <%= f.password_field :password, class: 'pswd', placeholder: 'Password', autocomplete: "off" %>
        <%= f.submit 'Ingresar', class: 'submit' %>

        <% if devise_mapping.rememberable? -%>
          <%= f.check_box :remember_me %>
          <%= f.label :remember_me, class: 'remember' %><br />
        <% end -%>

        <%= render "devise/shared/links" %>
      <% end %>
    </div>
  </div>
</div>
      